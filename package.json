{
  "name": "11ty-donnie-darko",
  "version": "1.0.0",
  "description": "Donnie Darko",
  "scripts": {
    "clean": "trash \"./docs/\"",
    "watch:eleventy": "npx eleventy --serve",
    "watch:tailwind": "npx tailwindcss -i ./src/css/tailwind.css -o ./docs/css/tailwind.css --watch",
    "watch": "run-p watch:*",
    "build": "NODE_ENV=production npm run-s clean css && NODE_ENV=production npx eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-fetch": "^4.0.0",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.1",
    "@11tyrocks/eleventy-plugin-lightningcss": "^1.2.0",
    "alpinejs": "^3.13.2",
    "clean-css": "^5.3.2",
    "concurrently": "^8.2.2",
    "del": "^7.1.0",
    "dotenv": "^16.3.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.4",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.3.3",
    "trash-cli": "^5.0.0"
  }
}
